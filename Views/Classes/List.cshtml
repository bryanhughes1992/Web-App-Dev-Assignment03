@model IEnumerable<ASSIGNMENT03_BRYANHUGHES.Models.Classes>
@{
    ViewBag.Title = "List";
}

<div id="listContainer">
    <h2>List</h2>
    <form action="/Classes/List" method="get">
        <input type="text" name="SearchKey" autocomplete="off" />
        <input type="submit" value="Search" />
    </form>

    @foreach (var teacher in Model)
    {
        <div class="listitem row">
            <div class="col-md-12">
                <a href="/Classes/Show/@Classes.ClassId"> @teacher.TeacherLname</a>
            </div>
        </div>
    }
</div>

