@Model ASSIGNMENT03_BRYANHUGHES.Models.Teacher
@{
    ViewBag.Title = "Show";
}

<div id="containershow">
    <h2>@Model.TeacherFname @Model.TeacherLname</h2>
    <p>Teacher ID: @Model.TeacherId</p>
    <p>Employee Number: @Model.EmployeeNumber</p>
    <p>Hire Date: @Model.HireDate</p>
    <p>Salary: $@Model.Salary / hour</p>

    <form action="/Teacher/ConfirmDelete/@Model.TeacherId" method="POST">
        <button>Delete</button>
    </form>
</div>

